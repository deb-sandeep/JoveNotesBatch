#!/bin/bash

JAVA_HOME=/usr/lib/jvm/jdk
SCRIPTDIR=.

CP="$SCRIPTDIR/config:$SCRIPTDIR/lib/avalon-framework-4.1.3.jar:$SCRIPTDIR/lib/c3p0-0.9.1.1.jar:$SCRIPTDIR/lib/commons-beanutils-1.7.0.jar:$SCRIPTDIR/lib/commons-beanutils-core-1.7.0.jar:$SCRIPTDIR/lib/commons-cli-1.3.jar:$SCRIPTDIR/lib/commons-codec-1.9.jar:$SCRIPTDIR/lib/commons-collections-3.2.jar:$SCRIPTDIR/lib/commons-configuration-1.5.jar:$SCRIPTDIR/lib/commons-digester-1.8.jar:$SCRIPTDIR/lib/commons-io-2.4.jar:$SCRIPTDIR/lib/commons-lang-2.3.jar:$SCRIPTDIR/lib/commons-logging-1.1.jar:$SCRIPTDIR/lib/commons-pool-1.6.jar:$SCRIPTDIR/lib/gson-2.8.0.jar:$SCRIPTDIR/lib/httpclient-4.4.1.jar:$SCRIPTDIR/lib/httpcore-4.4.1.jar:$SCRIPTDIR/lib/jcommon-1.0.23.jar:$SCRIPTDIR/lib/jfreechart-1.0.19.jar:$SCRIPTDIR/lib/jnbatch-1.0.jar:$SCRIPTDIR/lib/junit-4.4.jar:$SCRIPTDIR/lib/log4j-1.2.14.jar:$SCRIPTDIR/lib/logkit-1.0.1.jar:$SCRIPTDIR/lib/mysql-connector-java-8.0.17.jar:$SCRIPTDIR/lib/protobuf-java-3.6.1.jar:$SCRIPTDIR/lib/quartz-2.2.3.jar:$SCRIPTDIR/lib/servlet-api-2.3.jar:$SCRIPTDIR/lib/slf4j-api-1.7.5.jar:$SCRIPTDIR/lib/slf4j-log4j12-1.7.5.jar"

cd /home/sandeep/projects/bin/jnbatch

$JAVA_HOME/bin/java -classpath $CP com.sandy.jovenotes.jnbatch.JoveNotesBatch --runMode production